<h1>Users</h1>
<table class='table table-striped'>
    <tr>
        <th>ID</th>
        <td>Email</td>
        <th>Name</th>
    </tr>
    {{#each controller}}
        <tr>
            <td>{{id}}</td>
            <td>{{email}}</td>
            <td><a href='#'>{{fullName}}</a></td>
        </tr>
    {{/each}}
</table>