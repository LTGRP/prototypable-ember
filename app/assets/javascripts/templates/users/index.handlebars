<h1>Users</h1>
<table class='table table-striped'>
    <tr>
        <th>ID</th>
        <td>Email</td>
        <th>Name</th>
    </tr>
    {{#each user in users}}
        <tr>
            <td>{{user.id}}</td>
            <td>{{user.email}}</td>
            <td>{{#linkTo 'users.show' user}}{{user.fullName}}{{/linkTo}}</td>
        </tr>
    {{/each}}
</table>